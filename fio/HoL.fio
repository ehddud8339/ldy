[global]
directory=/mnt/test
#filename_format=fio_$jobnum
#numjobs=32
#numjobs=1
ioengine=psync
iodepth=1
direct=0
norandommap
group_reporting=1
#time_based=1
#runtime=30

[prepare-job]
rw=write
size=1M
bs=1M
numjobs=1000
filename_format=testfile_$jobnum
end_fsync=1

[big-job]
rw=write
size=4G
bs=512K
numjobs=1
filename_format=big_dummy_$jobnum
cpus_allowed=0
fsync=1

[small-job]
rw=randread
size=4G
bs=4K
numjobs=1
filename_format=small_dummy_$jobnum
