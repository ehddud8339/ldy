[global]
directory=/mnt/test
filename_format=testfile_$jobnum
#filename=/mnt/test/dummyfile
#numjobs=32
#numjobs=1
ioengine=psync
iodepth=1
direct=0
norandommap
group_reporting=1

[prepare-job]
bs=128K
rw=write
#size=4G
end_fsync=0

[target-job]
#bs=4K
rw=write
#size=4G
end_fsync=1
#time_based=1
#runtime=60

[test-job]
numjobs=1
